{
  "name": "urlparamify",
  "version": "1.1.4",
  "description": "Simply parse urls, no matter how stupid it looks. Focuses on coverage rather than features",
  "main": "./dist/index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "assert": "1.1.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^1.18.2",
    "webpack": "^2.6.1"
  },
  "scripts": {
    "test": "mocha dist --reporter spec",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "build": "webpack",
    "transpile": "babel src --presets babel-preset-es2015 --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaustubhhiware/urlparamify.git"
  },
  "keywords": [
    "url",
    "parse",
    "KISS"
  ],
  "author": "Kaustubh Hiware <hiwarekaustubh@googlemail.com> (http://kaustubhhiware.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaustubhhiware/urlparamify/issues"
  },
  "homepage": "https://github.com/kaustubhhiware/urlparamify#readme",
  "dependencies": {
    "build-url": "^1.0.9"
  }
}
